@model List<ClassType>

<div class="get-div">
    <h1 class="table-title">Class Type</h1>

    <div style="margin-top: 20px;">
    <a class="menu-item-btn" href="/Class/Get">Class</a>
    <a class="menu-item-btn" href="/ClassType/Get">Class Type</a>
    <a class="menu-item-btn" href="/Schedule/Get">Schedule</a>
    <a class="menu-item-btn" href="/Student/Get">Student</a>
    <a class="menu-item-btn" href="/Subject/Get">Subject</a>
    <a class="menu-item-btn" href="/Teacher/Get">Teacher</a>
</div>

    <a class="menu-item-btn" style="margin-top: 10px;" href="/ClassType/Create">Create</a>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Description</th>
                <th>Update</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var classType in @Model)
            {
                <tr>
                    <td>@classType.Id</td>
                    <td>@classType.Name</td>
                    <td>@classType.Description</td>
                    <td><a href="/ClassType/Update?id=@classType.Id">Update</a></td>
                    <td><a href="/ClassType/Delete?id=@classType.Id">Delete</a></td>
                </tr>
            }
        </tbody>
    </table>
</div>

<p></p>
<p>&nbsp;</p>
<p>&nbsp;</p>
<p>&nbsp;</p>
<p>&nbsp;</p>